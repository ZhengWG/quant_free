# QuantFree 后端配置

# 服务器配置
HOST=0.0.0.0
PORT=3000
DEBUG=True

# AI服务配置 (deepseek / openai / claude)
AI_PROVIDER=deepseek

# DeepSeek API (推荐，https://platform.deepseek.com/)
DEEPSEEK_API_KEY=
DEEPSEEK_MODEL=deepseek-chat

# OpenAI API (可选)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4

# Claude API (可选)
CLAUDE_API_KEY=

# 行情数据API (可选，默认使用免费的新浪/腾讯接口)
TUSHARE_TOKEN=
ALPHA_VANTAGE_API_KEY=

# 券商API (可选，当前为模拟交易模式)
BROKER_API_URL=
BROKER_API_KEY=
BROKER_API_SECRET=

# 数据库
DB_PATH=./data/quant_free.db

# 日志
LOG_LEVEL=INFO
LOG_FILE=./logs/app.log
